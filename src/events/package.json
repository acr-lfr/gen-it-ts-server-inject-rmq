{
  "name": "notifications",
  "description": "notifications handler for the app, sms, push, email, etc",
  "version": "0.0.1",
  "private": true,
  "author": "<add your company or personal name here>",
  "scripts": {
    "build": "tsc && tsc-alias",
    "cli-script": "VERBOSE=true && node ./build/server.js -r ",
    "dev:build": "tsc-watch --preserveWatchOutput --onSuccess 'npx tsc-alias'",
    "dev:setup": "git config core.hooksPath githooks && cp -n .env.example .env",
    "dev:start": "tsc-watch --preserveWatchOutput --onSuccess 'sh -c \"npx tsc-alias && npm start\"'",
    "jest": "node --random-seed=42 node_modules/.bin/jest",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "pm2:start:dev": "pm2 start pm2.dev.config.js",
    "pm2:start:prod": "pm2 start pm2.prod.config.js",
    "start": "node ./build/server.js",
    "test": "npm run build && npm run lint && npm run test:unit",
    "test:unit": "ENVIRONMENT=test && npm run jest --silent --runInBand",
    "test:unit:single": "npm run jest --clearCache && ENVIRONMENT=test && npm run jest --collect-coverage false --verbose --forceExit --detectOpenHandles",
    "generate:rabbitmq": "generate-it ../../rabbitmq/asyncapi/release/event_spec.yml -y -t https://github.com/acrontum/generate-it-asyncapi-rabbitmq-fanout.git -o src/events",
    "generate:server": "generate-it ../api_spec/release/api_spec_1.0.0.yml -m -y -u -t https://github.com/acr-lfr/gen-it-ts-server-inject-rmq.git"
  },
  "dependencies": {
    "celebrate": "^15.0.0",
    "command-line-args": "^5.2.0",
    "cors": "^2.8.5",
    "custom-error": "^0.2.1",
    "dotenv": "^14.2.0",
    "express": "^4.18.2",
    "express-auth-middle": "^1.1.2",
    "express-form-data": "^2.0.17",
    "express-promise-router": "^4.1.1",
    "generate-it-logger": "^1.2.7",
    "generate-it-mockers": "^1.1.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "node-worker-threads-pool": "^1.5.1",
    "object-reduce-by-map": "^2.1.0",
    "openapi-nodegen-config-helper": "^1.3.1",
    "recursive-readdir-sync": "^1.0.6",
    "request-ip": "^3.3.0",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.3.1",
    "winston": "^3.4.0",
    "worker-farm": "^1.7.0",
    "yamljs": "^0.3.0",
    "amqplib": "^0.10.3",
    "async-redis-shared": "^0.2.1",
    "distributed-systems-cache": "^1.14.0",
    "proc-env-helper": "^1.0.3",
    "q-wrapper": "^1.0.4"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.15",
    "@types/express-form-data": "^2.0.2",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.171",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.18",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.6.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.4",
    "prettier": "^2.8.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.1.3",
    "tsc-alias": "^1.8.2",
    "tsc-watch": "^6.0.0",
    "typescript": "~4.9.4",
    "@types/amqplib": "^0.10.0",
    "generate-it": "^5.53.0"
  },
  "mockingServer": true
}
